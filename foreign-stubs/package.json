{
  "name": "pesy-example",
  "esy": {
    "build": "dune build -p pesy-example"
  },
  "buildDirs": {
    "src": {
      "bin": "Main.re",
      "imports": [
        "Library = require('./lib')"
      ]
    },
    "src/lib": {
      "foreignStubs": [
        {
          "language": "c",
          "names": [
            "is-prime",
            "sieve"
          ],
          "flags": ["-lm"]
        }
      ]
    }
  },
  "scripts": {
    "start": "esy x Main.exe"
  },
  "dependencies": {
    "@opam/dune": "2.8.0",
    "ocaml": "4.12.x",
    "@pesy/esy-pesy": "0.1.0-alpha.14"
  },
  "resolutions": {
        "@esy-ocaml/reason": "github:reasonml/reason#ccc34729994b4a80d4f6274cc0165cd9113444d6"
  }
}
